{
  "name": "ai-services",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint . --ext .vue,.js,.cjs,.ts",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "precommit": "npm audit && npm run lint",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "openai": "^4.28.4",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@gge/eslint-config-epsvue": "github:IT-WIBRC/eslint-config-epsvue",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.18",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}
